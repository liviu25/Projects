            <TableColumn fx:id="statusColumn" text="Status" />
        </columns>
      </TableView>
      <Label layoutX="377.0" layoutY="44.0" text="Denumire" />
      <Label layoutX="379.0" layoutY="122.0" text="Descriere" />
      <TextField fx:id="denumireText" layoutX="377.0" layoutY="75.0" prefHeight="31.0" prefWidth="266.0" />
      <TextArea fx:id="descriereText" layoutX="372.0" layoutY="145.0" prefHeight="200.0" prefWidth="276.0" />
      <Button layoutX="372.0" layoutY="389.0" mnemonicParsing="false" onAction="#selectHandler" text="Selectare bug" />
      <Button fx:id="removeButton" disable="true" layoutX="543.0" layoutY="389.0" mnemonicParsing="false" onAction="#removeHandler" text="Eliminare" />
